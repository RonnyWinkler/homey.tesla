          "min": 50,
          "max": 100,
          "step": 1,
          "label": "%",
          "labelDecimals": 1,
          "title": {
            "en": "Charge limit SoC",
            "de": "Ladelimit SoC",
            "nl": "Laadlimiet SoC"
                  },
          "placeholder": {
            "en": "Charge limit SoC",
